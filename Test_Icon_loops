def generate_binary():
    binary_array = []
    for i in range(2):
        for j in range(2):
            for k in range(2):
                for l in range(2):
                    binary_array.append([i,j,k,l])
    return binary_array

for index, list in enumerate(generate_binary()):
    print(f'{"|" * index}')
    print('\r')

def main():
          generate_binary()
          
          
if __name__ == "__main__":
    main()
